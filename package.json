{
  "name": "lyruanna",
  "version": "1.0.0",
  "description": "A modern BMS rhythm game player",
  "author": "Yumemizook",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.lyruanna.player",
    "productName": "Lyruanna",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": [
        "portable"
      ]
    },
    "files": [
      "**/*",
      "!dist/*"
    ]
  },
  "dependencies": {
    "ffmpeg-static": "^5.3.0",
    "fluent-ffmpeg": "^2.1.3",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.3",
    "iconv-lite": "^0.7.2"
  },
  "devDependencies": {
    "electron": "^25.0.0",
    "electron-builder": "^26.4.0"
  }
}